	<gallery id="CRRES/PWE">
		<title>CRRES/PWE</title>
		<aboutlink>http://www-pw.physics.uiowa.edu/plasma-wave/crres</aboutlink>
		<defaultfilter>11through20</defaultfilter>
		<fulldir>http://www-pw.physics.uiowa.edu/plasma-wave/crres/GIF/</fulldir>
		<thumbdir>http://www-pw.physics.uiowa.edu/plasma-wave/crres/GIF/thumbs400/</thumbdir>
		<fulllistscript>
			<![CDATA[
				(function () {
					var files = new Array();
					var attributes = attributemap("dat/crres-orbits.dat");
					for (i = 350; i < 500; i++) {
						files[i-350] = ["o" + sprintf('%04d',i) + ".gif",i,attributes[i][0],attributes[i][1]];
					}
					return files;						
				})
			]]>
		</fulllistscript>						
		<attributes>

			<attribute id="$0">
				<name>Filename</name>
				<filters>
					<filter>
						<title>All</title>
						<value>.*</value>
					</filter>
				</filters>
			</attribute>		

			<attribute>
				<name>Orbit</name>
				<type>integer</type>
				<filters>
					<filter id="11through20">
						<title>Orbit # between 10 and 20</title>
						<value>(this gt 10) and (this lt 20)</value>
					</filter>
				</filters>
			</attribute>
			
			<attribute id="$2">
				<name>Orbit Start Time</name>
				<type>time</type>
				<filters>
					<filter>
						<title>1990-08-02</title>
						<value>1990-08-02.*</value>
					</filter>
					<filter>
						<title>1990-08-03</title>
						<value>1990-08-03.*</value>
					</filter>
				</filters>
			</attribute>

			<attribute id="$3">
				<name>Orbit Stop Time</name>
				<type>time</type>
				<filters>
					<filter>
						<title>1990-08-02</title>
						<value>1990-08-02.*</value>
					</filter>
					<filter>
						<title>1990-08-03</title>
						<value>1990-08-03.*</value>
					</filter>
				</filters>
			</attribute>
			
		</attributes>
	</gallery>
