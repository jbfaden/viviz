	<gallery id="Test/1">
		<title>Test/1</title>
		<fullfiles>xml/Test-filelist.xml</fullfiles>
		<fulldir>http://viviz.org/demo/images/demo/full/</fulldir>
		<thumbdir>http://viviz.org/demo/images/demo/thumb/</thumbdir>	
		<attributes>
			<attribute id="$0">
				<name>Filename</name>
				<filters>
					<filter>
						<title>All</title>
						<value>.*</value>
					</filter>
					<filter>
						<title>T1.*</title>
						<value>T1.*</value>
					</filter>
				</filters>
			</attribute>		
			<attribute id="$1">
				<name>Duration</name>
				<unit>hour</unit>
				<format>%d</format>
				<filters>
					<filter>
						<title>All</title>
						<value>true</value>
					</filter>
					<filter>
						<title>Duration &gt; 10</title>
						<value>this gt 10</value>
					</filter>
				</filters>
			</attribute>
			<attribute>
				<name>Integral 1</name>
				<unit>nT&#183;hr</unit>
				<format>%.0f</format>
			</attribute>
			<attribute>
				<name>Integral 2</name>
				<unit>nT&#183;km/s&#183;hr</unit>
				<format>%.0f</format>
			</attribute>
			<attribute>
				<name>Integral 3</name>
				<unit>#/cc&#183;hr</unit>
				<format>%.0f</format>
			</attribute>
		</attributes>			
	</gallery>
