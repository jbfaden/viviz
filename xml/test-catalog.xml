	<gallery id="Test/7">
		<title>Test/7</title>
		<about>Test of fulllistscript</about>
		<aboutlink></aboutlink>
		<fulldir>images/full/</fulldir>
		<thumbdir>images/thumb/</thumbdir>
		<fulllistscript>
			<![CDATA[
				(function () {
					var files = new Array();
					for (i = 0; i < 4; i++) {
						files[i] = ["demo-" + sprintf('%04d',i+1) + ".png"];
					}
					return files;						
				})
			]]>
		</fulllistscript>						
